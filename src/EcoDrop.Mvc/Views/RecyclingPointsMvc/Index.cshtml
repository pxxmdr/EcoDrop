@model IEnumerable<EcoDrop.Mvc.Service.RecyclingPointDto>

<h2>Pontos de Reciclagem (OracleApi)</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Endere√ßo</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var point in Model)
    {
        <tr>
            <td>@point.Id</td>
            <td>@point.Name</td>
            <td>@point.Address</td>
        </tr>
    }
    </tbody>
</table>
